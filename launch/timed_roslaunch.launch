<launch>
  <arg name="time" default="0"/>
  <arg name="pkg" default=""/>
  <arg name="file" default=""/>
  <arg name="value" default="" />
  <arg name="node_name" default="timed_roslaunch_$(arg time)_$(arg pkg)" />

  <node pkg="atwifr_gazebo" type="timed_roslaunch.sh" args="$(arg time) $(arg pkg) $(arg file) $(arg value)" name="$(arg node_name)" output="screen" />
</launch>